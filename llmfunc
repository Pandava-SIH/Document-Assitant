import nltk
from nltk import sent_tokenize
import fitz  # PyMuPDF

def read_pdf(file_path):
    doc = fitz.open(file_path)
    text = ''

    for page_num in range(doc.page_count):
        page = doc[page_num]
        text += page.get_text()

    return text

# Example usage:
pdf_text = read_pdf(r"C:\Users\HP\Downloads\sodapdf-converted (1).pdf")
print(pdf_text)
